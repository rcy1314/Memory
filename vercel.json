{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/run.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "installCommand": "cd web && npm install && npm run build && cd .. && pip install -r requirements.txt",
  "buildCommand": "echo 'Build completed'",
  "outputDirectory": "dist"
}