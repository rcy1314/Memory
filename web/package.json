{
  "name": "moment-frontend",
  "version": "1.0.7",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --fix --ext .js,.vue .",
    "lint:staged": "lint-staged",
    "prettier": "npx prettier --write .",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "node -e \"require('fs').copyFileSync('.env.tauri', '.env.production')\" && tauri build",
    "tauri:android:init": "tauri android init",
    "tauri:android:dev": "tauri android dev",
    "tauri:android:build": "tauri android build",
    "tauri:android:build:aarch64": "tauri android build --target aarch64",
    "tauri:android:build:armv7": "tauri android build --target armv7",
    "tauri:android:build:x86_64": "tauri android build --target x86_64"
  },
  "dependencies": {
    "@iconify/json": "^2.2.101",
    "@iconify/vue": "^4.1.1",
    "@unocss/eslint-config": "^0.61.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^10.3.0",
    "@zclzone/eslint-config": "^0.0.4",
    "axios": "^1.4.0",
    "browser-image-compression": "^2.0.2",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "exif-js": "^2.3.0",
    "exifr": "^7.1.3",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.6",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.89.2",
    "typescript": "^5.1.6",
    "unocss": "^0.61.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.4.0",
    "vue-final-modal": "^4.5.5",
    "vue-i18n": "9",
    "vue-masonry-css": "^1.0.3",
    "vue-progressive-image": "^5.0.4",
    "vue-router": "^4.2.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/cli": "^2.7.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "vite": "^5.4.19",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --ext .js,.vue ."
    ]
  },
  "eslintConfig": {
    "extends": [
      "@zclzone",
      "@unocss",
      ".eslint-global-variables.json"
    ],
    "rules": {
      "vue/no-v-model-argument": "off",
      "vue/valid-v-model": "off"
    }
  }
}
